package com.company;
import java.lang.Math.*;
class Punkt
{
    public int x;
    public int y;

    public Punkt() //konstruktor
    {
        x=0;
        y=0;
    }
    public Punkt(int q, int p) //konstruktor 2
    {
        x=q;
        y=p;
    }
    public double odleglosc(int x2, int y2)
    {
        return Math.sqrt(Math.pow(x2-x,2)+Math.pow(y2-y,2));
    }
    public double odleglosc2(Punkt UwU)
    {
        return Math.sqrt(Math.pow(UwU.x-x,2)+Math.pow(UwU.y-y,2));
    }}
